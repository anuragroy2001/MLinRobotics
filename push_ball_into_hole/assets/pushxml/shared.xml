<mujoco>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.5 0.5 0.5" rgb2="0 0 0" width="32" height="32"></texture>

        <texture name="texture_ball" file="golf.png" type="2d" width="256" height="256"></texture>
        <texture name="texture_table" file="grass.png" type="2d" width="256" height="256"></texture>


        <material name="floor_mat" specular="0" texture= "texture_table" shininess="0.5" reflectance="0" rgba="0.7 0.9 0.7 1"></material>
        <material name="table_mat" specular="0" texture= "texture_table" shininess="0.5" reflectance="0" rgba="0.7 0.9 0.7 1"></material>
        <material name="ball_mat" texture= "texture_ball" specular="0" shininess="0.5" reflectance="0" rgba="1 1 1 1"></material>
        <material name="robot0:geomMat" shininess="0.03" specular="0.4"></material>
        <material name="robot0:gripper_finger_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        <material name="robot0:gripper_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        <material name="robot0:arm_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        <material name="robot0:head_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        <material name="robot0:base_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        
        <mesh file="base_link.STL" name="robot0:base_link"></mesh>
        <mesh file="link_1.STL" name="robot0:link_1"></mesh>
        <mesh file="link_2.STL" name="robot0:link_2"></mesh>
        <mesh file="link_3.STL" name="robot0:link_3"></mesh>
        <mesh file="link_4.STL" name="robot0:link_4"></mesh>
        <mesh file="link_5.STL" name="robot0:link_5"></mesh>
        <mesh file="link_6.STL" name="robot0:link_6"></mesh>
        <mesh file="link_7.STL" name="robot0:link_7"></mesh>
        <mesh file= "gripper2.stl" name="robot0:gripper"></mesh>
    </asset>

    <equality>
        <weld body1="robot0:mocap" body2="robot0:r_gripper_finger_link" solimp="0.9 0.95 0.001" solref="0.02 1"></weld>
    </equality>

    <contact>
        <exclude body1="robot0:r_gripper_finger_link" body2="robot0:l_gripper_finger_link"></exclude>
    </contact>
    
    <default>
        <default class="robot0:fetch">
            <geom margin="0.001" material="robot0:geomMat" rgba="1 1 1 1" solimp="0.99 0.99 0.01" solref="0.01 1" type="mesh" user="0"></geom>
            <joint armature="1" damping="50" frictionloss="0" stiffness="0"></joint>
            
            <default class="robot0:fetchGripper">
                <geom condim="4" margin="0.001" type="box" user="0" rgba="0.356 0.361 0.376 1.0"></geom>
                <joint armature="100" damping="1000" limited="true" solimplimit="0.99 0.999 0.01" solreflimit="0.01 1" type="slide"></joint>
            </default>

            <default class="robot0:grey">
                <geom rgba="0.2 0.2 0.2 1.0"></geom>
            </default>
        </default>
    </default>
</mujoco>

